# ==================================================
# Node / React ignores
# ==================================================

# dependencies (ignore node_modules anywhere)
node_modules/

# production builds
dist/
build/

# misc
.DS_Store
Thumbs.db

# local env files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# logs
npm-debug.log*
yarn-debug.log*
pnpm-debug.log*

